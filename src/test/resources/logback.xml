<?xml version="1.0" encoding="UTF-8"?>

<configuration>
	<statusListener
		class="ch.qos.logback.core.status.NopStatusListener" />
	<appender name="STDOUT"
		class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%-5level %c{0}.%M\(\)- %msg%n</pattern>
		</encoder>
	</appender>

	<root level="debug">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>